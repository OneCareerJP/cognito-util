{
  "name": "cognito-util",
  "version": "0.0.1",
  "description": "Helpers that make working with AWS Cognito easier",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --write '**/*.{js,ts,vue,json,scss}'",
    "watch:prettier": "onchange '**/*.{js,ts,vue,json,scss}' -- prettier --write {{changed}}"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "aws",
    "cognito"
  ],
  "author": "shskwmt",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "query-string": "^6.13.5"
  }
}
